{
  "name": "wp-flawless",
  "repo": "UsabilityDynamics/wp-flawless",
  "version": "0.1.1",
  "description": "Flawless Theme.",
  "scripts": [
    "ux/scripts/core.js",
    "ux/scripts/app.js"
  ],
  "styles": [
    "ux/styles/core.less",
    "ux/styles/content.less",
    "ux/styles/mixins.less",
    "ux/styles/app.less"
  ],
  "templates": [
    "ux/templates/template.html"
  ],
  "images": [
    "ux/images/ajax-loader.gif",
    "ux/images/no-skin-thumbanil.gif",
    "ux/images/fancybox-sprite.png",
    "ux/images/flawless-default-logo.png",
    "ux/images/glyphicons-halflings-white.png",
    "ux/images/glyphicons-halflings.png"
  ],
  "config": {
    "component-dir": "components",
    "output": {
      "js": "ux/scripts/vendor",
      "css": "ux/styles/vendor",
      "less": "ux/styles/vendor",
      "fonts": "ux/fonts/vendor",
      "template": "ux/templates/vendor",
      "php": "core/vendor"
    }
  },
  "dependencies": {
    "usabilitydynamics/lib-api": "*",
    "usabilitydynamics/lib-loader": "*",
    "usabilitydynamics/lib-ui": "*",
    "usabilitydynamics/lib-utility": "*",
    "usabilitydynamics/lib-settings": "*",
    "usabilitydynamics/lib-saas": "*",
    "usabilitydynamics/lib-models": "*",
    "usabilitydynamics/lib-cdn": "*",

    "usabilitydynamics/js-mvvm": "*",
    "usabilitydynamics/js-spa": "*",
    "usabilitydynamics/js-elastic-filter": "*",

    "kaerus-component/hash": "*",
    "component/model": "*",
    "component/s3": "*",
    "mikanda/schema-validate": "*",
    "camshaft/bootstrap-responsive": "*",
    "camshaft/bootstrap-grid": "*",
    "camshaft/bootstrap-buttons": "*",
    "camshaft/bootstrap-navbar": "*",
    "camshaft/bootstrap-dropdowns": "*",
    "camshaft/bootstrap-thumbnails": "*",
    "camshaft/bootstrap-forms": "*",
    "camshaft/bootstrap-tables": "*",
    "camshaft/bootstrap-reset": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UsabilityDynamics/wp-flawless"
  },
  "license": "MIT"
}