  "homepage": "https://github.com/UsabilityDynamics/wp-amd",
  "keywords": [
    "amd",
    "async",
    "wordpress",
    "scripts",
    "styles"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Andy Potanin",
