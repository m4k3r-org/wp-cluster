{
  "name": "wpCloud/wp-vertical-edm",
  "type": "wordpress-plugin",
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "github-protocols": [ "ssh" ],
    "vendor-dir": "vendor/libraries",
    "bin-dir": "vendor/bin",
    "optimize-autoloader": true
  },
  "require": {
    "composer/installers": "~1.0",
    "usabilitydynamics/lib-utility": "~0.3",
    "usabilitydynamics/lib-model": "~0.3"
  },
  "autoload": {
    "classmap": [ "lib" ]
  },
  "extra": {
    "installer-paths": {
      "application/vendor/{$name}": [
        "type:wpcloud-application"
      ],
      "storage/vendor/{$name}": [
        "type:wpcloud-storage"
      ],
      "vendor/libraries/{$name}": [
        "type:wordpress-library"
      ],
      "vendor/modules/{$name}": [
        "type:wordpress-muplugin",
        "type:wordpress-plugin"
      ],
      "vendor/themes/{$name}": [
        "type:wordpress-theme"
      ]
    }
  },
  "minimum-stability": "dev",
  "license": "GPLv2"
}
